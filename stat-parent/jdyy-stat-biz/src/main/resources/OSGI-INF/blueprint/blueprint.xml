<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.2.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
">
    <tx:enable-annotations/>

    <reference id="statBeanDao" interface="com.kalix.jdyy.stat.api.dao.IStatBeanDao"/>

    <service id="statBeanService" interface="com.kalix.jdyy.stat.api.biz.IStatBeanService"
             ref="statBeanServiceImpl"/>

    <bean id="statBeanServiceImpl" class="com.kalix.jdyy.stat.biz.StatBeanServiceImpl">
        <property name="dao" ref="statBeanDao"/>
    </bean>


</blueprint>

