<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.2.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
">
    <tx:enable-annotations/>
    <!--柱状图数据-->
    <reference id="cascadeBeanDao" interface="com.kalix.jdyy.cascade.api.dao.ICascadeBeanDao"/>

    <service id="cascadeBeanService" interface="com.kalix.jdyy.cascade.api.biz.ICascadeBeanService"
             ref="cascadeBeanServiceImpl"/>

    <bean id="cascadeBeanServiceImpl" class="com.kalix.jdyy.cascade.biz.CascadeServiceImpl">
        <property name="dao" ref="cascadeBeanDao"/>
    </bean>

</blueprint>

